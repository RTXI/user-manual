@article{Sherman:1999p1234,
author = {A J Sherman and A Shrier and E Cooper}, 
journal = {Biophys J},
title = {Series resistance compensation for whole-cell patch-clamp studies using a membrane state estimator},
abstract = {Whole-cell patch-clamp techniques are widely used to measure membrane currents from isolated cells. While suitable for a broad range of ionic currents, the series resistance (R(s)) of the recording pipette limits the bandwidth of the whole-cell configuration, making it difficult to measure rapid ionic currents. To increase bandwidth, it is necessary to compensate for R(s). Most methods of R(s) compensation become unstable at high bandwidth, making them hard to use. We describe a novel method of R(s) compensation that overcomes the stability limitations of standard designs. This method uses a state estimator, implemented with analog computation, to compute the membrane potential, V(m), which is then used in a feedback loop to implement a voltage clamp; we refer to this as state estimator R(s) compensation. To demonstrate the utility of this approach, we built an amplifier incorporating state estimator R(s) compensation. In benchtop tests, our amplifier showed significantly higher bandwidths and improved stability when compared with a commercially available amplifier. We demonstrated that state estimator R(s) compensation works well in practice by recording voltage-gated Na(+) currents under voltage-clamp conditions from dissociated neonatal rat sympathetic neurons. We conclude that state estimator R(s) compensation should make it easier to measure large rapid ionic currents with whole-cell patch-clamp techniques.},
