@article{Mason:2005p4413,
author = {M J Mason and A K Simpson and M P Mahaut-Smith and H P C Robinson}, 
journal = {Biophys J},
title = {The interpretation of current-clamp recordings in the cell-attached patch-clamp configuration},
abstract = {In these experiments we have investigated the feasibility and accuracy of recording steady-state and dynamic changes in transmembrane potential noninvasively across an intact cell-attached patch using the current-clamp mode of a conventional patch-clamp amplifier. Using an equivalent circuit mimicking simultaneous whole-cell voltage-clamp and cell-attached current-clamp recordings we have defined both mathematically and experimentally the relationship between the membrane patch resistance, the seal resistance, and the fraction of the whole-cell potential recorded across an intact membrane patch. This analysis revealed a steep increase in the accuracy of recording of steady-state membrane potential as the seal/membrane ratio increases from 0. The recording accuracy approaches 100% as the seal/membrane ratio approaches infinity. Membrane potential measurements across intact cell-attached patches in rat basophilic leukemia cells and rat megakaryocytes revealed a surprisingly high degree of accuracy and demonstrated the ability of this noninvasive technique to follow dynamic changes in potential in nonexcitable cells.},
