@article{Shin:2006p1669,
author = {S Shin and Erik De Schutter}, 
journal = {J Neurophysiol},
title = {Dynamic synchronization of Purkinje cell simple spikes},
abstract = {Purkinje cells ( PCs) integrate all computations performed in the cerebellar cortex to inhibit neurons in the deep cerebellar nuclei ( DCN). Simple spikes recorded in vivo from pairs of PCs separated by < 100 mu m are known to be synchronized with a sharp peak riding on a broad peak, but the significance of this finding is unclear. We show that the sharp peak consists exclusively of simple spikes associated with pauses in firing. The broader, less precise peak was caused by firing-rate co-modulation of faster firing spikes. About 13% of all pauses were synchronized, and these pauses had a median duration of 20 ms. As in vitro studies have reported that synchronous pauses can reliably trigger spikes in DCN neurons, we suggest that the subgroup of spikes causing the sharp peak is important for precise temporal coding in the cerebellum.},
